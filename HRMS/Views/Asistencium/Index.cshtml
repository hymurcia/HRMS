@model IEnumerable<HRMS.Models.Asistencium>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Entrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Calificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCedulaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Entrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Calificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCedulaNavigation.Cedula)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCedula">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCedula">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCedula">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
